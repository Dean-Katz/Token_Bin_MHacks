{
   "action": {
      "default_icon": {
         "128": "128.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "worker.js"
   },
   "content_scripts": [
      {
         "js": [
            "content.js"
         ],
         "matches": [
            "*://*.openai.com/*"
         ]
      }
   ],
   "permissions": [
      "webRequest",
      "nativeMessaging",
      "tabs",
      "clipboardWrite",
      "scripting"
   ],
   "host_permissions": [
      "https://chat.openai.com/*"
   ],
   "externally_connectable": {
      "matches": [
         "*://*.openai.com/*"
      ]
   },
   "description": "a description",
   "icons": {
      "128": "128.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "ChatGPT Power Tools",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1",
   "devtools_page": "devtools.html"
}